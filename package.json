{
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.1",
    "husky": "8",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "husky install"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "bracketSameLine": true,
    "jsxSingleQuote": true,
    "bracketSpacing": false,
    "endOfLine": "auto"
  }
}
